package jp.co.internous.action;

public class Person {
	public String name = null;
	public int age =0;
	public String phoneNumber=null;
	public String address=null;
	public String gender=null;

	public void talk(){
		System.out.println(this.talk + "が話す");
	}

	public void walk(){
		System.out.println(this.walk + "が歩く");
	}

	public void run(){
		System.out.println(this.run + "が走る");
	}

}
